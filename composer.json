{
  "name": "koempf/pixi-client",
  "description": "",
  "require": {
    "php": "^7.1",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-soap": "*",
    "symfony/yaml": "~3.4|~4.0",
    "webmozart/assert": "*",
    "symfony/serializer": "~3.4|~4.0"
  },
  "require-dev": {
